<Window x:Class="WindowzTabManager.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="設定"
        Width="960"
        Height="700"
        MinWidth="820"
        MinHeight="560"
        WindowStartupLocation="CenterOwner"
        Background="{DynamicResource ApplicationBackgroundBrush}"
        Foreground="{DynamicResource TextFillColorPrimaryBrush}">
    <Grid Margin="16">
        <TabControl x:Name="SettingsTabControl"
                    Background="{DynamicResource ApplicationBackgroundBrush}"
                    BorderBrush="{DynamicResource ControlStrokeColorDefaultBrush}">
            <TabItem x:Name="GeneralTab" Header="General" />
            <TabItem x:Name="HotkeyTab" Header="Hotkey" />
            <TabItem x:Name="StartupTab" Header="Startup" />
            <TabItem x:Name="QuickLaunchTab" Header="Quick Launch" />
            <TabItem x:Name="ProcessInfoTab" Header="Process Info" />
        </TabControl>
    </Grid>
</Window>
